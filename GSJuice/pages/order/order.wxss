/* pages/order/order.wxss */
page{
  width: 100%;
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
  background-color: #EAEAEA;
}
.order {
  position: absolute;
  top: 15%;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
  width: 90%;
  height: 80%;
  background-color: #FFFFFF;
  border-radius: 20rpx;
}
.no-order{
  width: 100%;
  height: 100%;
  text-align: center;
}
.figure{
  width: 100%;
  height: 80%;
  padding: 20%;
  box-sizing: border-box;
}
.no-image{
  margin-top: 20%;
  width: 250rpx;
  height: 350rpx;
}
.image-title{
  color: #999999;
  font-size: 28rpx;
}
.btn-group{
  width: 100%;
  height: 20%;
}
.go-buy{
  width: 380rpx;
  height: 90rpx;
  background-color: #D6A874;
  color: #FFFFFF;
  line-height: 90rpx;
  margin: 10rpx auto;
  font-size: 35rpx;
}
.history{
  margin-top: 50rpx;
  font-size: 28rpx;
  color: #A1A1A1;
}

